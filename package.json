{
  "name": "rn-optimisation-with-callstack",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "flashlight:measure": "flashlight measure",
    "flashlight:test:android": "flashlight test --bundleId com.yushan_blott.rnoptimisationwithcallstack --testCommand \"yarn android\" --duration 10000",
    "flashlight:test:ios": "flashlight test --bundleId com.yushan-blott.rnoptimisationwithcallstack --testCommand \"yarn ios\" --duration 10000",
    "flashlight:startup:android": "flashlight test --bundleId com.yushan_blott.rnoptimisationwithcallstack --testCommand \"./flashlight-tests/app-startup.sh android\" --duration 10000 --resultsFilePath ./flashlight-results/startup-android.json",
    "flashlight:startup:ios": "flashlight test --bundleId com.yushan-blott.rnoptimisationwithcallstack --testCommand \"./flashlight-tests/app-startup.sh ios\" --duration 10000 --resultsFilePath ./flashlight-results/startup-ios.json",
    "flashlight:profiling:android": "flashlight test --bundleId com.yushan_blott.rnoptimisationwithcallstack --testCommand \"./flashlight-tests/profiling-screen-test.sh android\" --duration 15000 --resultsFilePath ./flashlight-results/profiling-android.json",
    "flashlight:report": "flashlight report ./flashlight-results/results.json"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "babel-plugin-react-compiler": "^19.0.0-beta-af1b7da-20250417",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "expo": "~53.0.20",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "typescript": "~5.8.3",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0"
  },
  "private": true
}
